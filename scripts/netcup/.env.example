NETCUP_REFRESH_TOKEN=

TELEGRAM_BOT_TOKEN=123123123:asdasdasdasdasd
TELEGRAM_CHAT_ID=123456789

DEBUG=yes

# Avoid inline comments unless the value is quoted.
# Good:
#   SERVER_NAME=v123123123123
#   SERVER_NAME="v123123123123"  # comment ok (quoted)
# Bad (comment becomes part of the value in many parsers):
#   SERVER_NAME=v123123123123  # myhost.example.com
SERVER_NAME=v123123123123

# Required if you want `--monitor` + SSH attach/tailing across reboots.
# Use an explicit key path (the script refuses to use default identities).
NETCUP_SSH_IDENTITY_FILE=~/.ssh/id_ed25519
